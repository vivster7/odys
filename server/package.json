{
  "name": "server",
  "version": "1.0.0",
  "private": true,
  "description": "Boxes and Arrows server",
  "main": "src/index.js",
  "scripts": {
    "build-client": "cd ../client && yarn && yarn build && mv -f build ../server/build/client",
    "build-production": "rm -rf build && yarn && tsc && yarn build-client",
    "deploy-production": "yarn build-production && gcloud app deploy --quiet",
    "start-dev": "ts-node-dev --poll --respawn --transpileOnly src/app.ts",
    "start": "node ./build/app.js"
  },
  "repository": {
    "type": "git",
    "url": "odys"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.6",
    "@types/helmet": "^0.0.47",
    "@types/socket.io": "^2.1.4",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.23.1",
    "socket.io": "^2.3.0",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.3",
    "ts-node-dev": "^1.0.0-pre.44"
  }
}
